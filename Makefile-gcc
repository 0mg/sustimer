CL = gcc $(@:.exe=.c) -o $@ -mwindows -s -nostartfiles -lole32 -lpowrprof -lwinmm
DL = gcc $(@:.dll=.c) -o $@ -shared -s

.PHONY: all
all: sustimer.exe suspend.exe dispoff.exe dblckspd.exe powrprof.dll

sustimer.exe:
	$(CL)

suspend.exe:
	$(CL)

dispoff.exe:
	$(CL)

dblckspd.exe:
	$(CL)

powrprof.dll:
	$(DL)
